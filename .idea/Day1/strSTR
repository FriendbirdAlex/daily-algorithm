class Solution {
    public int strStr(String haystack, String needle) {
int n = haystack.length();
        int m = needle.length();
        for (int i = 0; i <= n - m; i++) {
            String sub = haystack.substring(i, i + m);

            // 3. 比较内容是否相等
            if (sub.equals(needle)) {
                return i;
            }
        }
         return -1;
    }

}